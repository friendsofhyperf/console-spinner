{
    "name": "friendsofhyperf/console-spinner",
    "description": "Custom progress bar component For Hyperf",
    "type": "library",
    "require": {
        "hyperf/command": "~2.2.0",
        "hyperf/di": "~2.2.0",
        "symfony/console": "^5.3",
        "symfony/process": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/framework": "~2.2.0",
        "hyperf/ide-helper": "~2.2.0",
        "phpstan/phpstan": "^0.12",
        "swoole/ide-helper": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfHyperf\\ConsoleSpinner\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "FriendsOfHyperf\\ConsoleSpinner\\ConfigProvider"
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./src"
    }
}